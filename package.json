{
  "name": "zod-to-schema",
  "version": "1.0.2",
  "description": "Convert Zod schemas to Mongoose and Prisma schemas while maintaining type safety and relationships",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "zod",
    "mongoose",
    "prisma",
    "schema",
    "typescript",
    "validation",
    "orm",
    "database"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonit-dev/zod-to-schema"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "mongoose": "^8.8.0",
    "tsup": "^8.3.5",
    "zod": "^3.23.8"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "wrap-ansi": "7.0.0"
  },
  "peerDependencies": {
    "mongoose": ">=7.0.0",
    "zod": ">=3.0.0"
  }
}
